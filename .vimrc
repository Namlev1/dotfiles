set scrolloff=5
set relativenumber
set clipboard^=unnamed
" Do incremental searching.
set incsearch

noremap <c-d> <c-d>zz
noremap <c-u> <c-u>zz
"" Keymaps for number row in normal mode
noremap # 1
noremap [ 2
noremap { 3
noremap } 4
noremap ( 5
noremap & 6
noremap * 7
noremap ) 8
noremap + 9
noremap ] 0
noremap 1 #
noremap 2 [
noremap 3 {
noremap 4 }
noremap 5 (
noremap 6 &
noremap 7 *
noremap 8 )
noremap 9 +
noremap 0 ]

noremap x "_x

" let mapleader = " "
" noremap <leader>y "+y
set scrolloff=5
